{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@constant/*": ["constants/*"],
      "@context/*": ["context/*"],
      "@hook/*": ["hooks/*"],
      "@store/*": ["store/*"],
      "@util/*": ["utils/*"],
      "@element/*": ["components/elements/*"],
      "@layout/*": ["components/layouts/*"],
      "@module/*": ["components/modules/*"],
      "@template/*": ["components/templates/*"],
      "@lib/*": ["../lib/*"],
      "@provider/*": ["../providers/*"]
    }
  }
}
