{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@api/*": ["api/*"],
      "@constant/*": ["constants/*"],
      "@context/*": ["context/*"],
      "@element/*": ["components/elements/*"],
      "@hook/*": ["hooks/*"],
      "@layout/*": ["components/layouts/*"],
      "@lib/*": ["../lib/*"],
      "@module/*": ["components/modules/*"],
      "@provider/*": ["../providers/*"],
      "@template/*": ["components/templates/*"],
      "@util/*": ["utils/*"]
    }
  }
}
